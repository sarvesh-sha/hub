{
    "compilerOptions": {
        "baseUrl": "",
        "declaration": false,
        "experimentalDecorators": true,
        "lib": [
            "es6",
            "dom"
        ],
        "mapRoot": "./",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "../dist/out-tsc",
        "sourceMap": true,
        "target": "es5",
        "typeRoots": [
            "../node_modules/@types"
        ],
        "downlevelIteration": true
    },
    "angularCompilerOptions": {
        "preserveWhitespaces": false,
        "fullTemplateTypeCheck": true
    },
    "files": [
        "main.ts",
        "polyfills.ts",
        "tableau.d.ts"
    ]
}
